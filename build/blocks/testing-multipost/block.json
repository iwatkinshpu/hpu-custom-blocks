  "category": "theme",
  "icon": "smiley",
  "description": "Testing stuff...",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "testing-multipost",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
